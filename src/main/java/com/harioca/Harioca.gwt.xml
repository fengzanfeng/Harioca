<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Harioca">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name='com.mastergaurav.codemirror.CodeMirror'/>
    <inherits name="com.smartgwt.SmartGwt"/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/HariocaShellService' class='com.harioca.server.HariocaShellServiceImpl'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.harioca.client.ui.torefactor.HariocaEntryPoint'/>

    <set-property name="user.agent" value="safari"/>

</module>